-@@ -758,8 +758,6 @@
- EOF
-         ;;
-     esac
--    # libwebm needs to be linked with C++ standard library
--    enabled webm_io && LD=${CXX}
- 
-     # append any user defined extra cflags
-     if [ -n "${extra_cflags}" ] ; then
diff --git a/contrib/src/vpx/rules.mak b/contrib/src/vpx/rules.mak
